<%= form_for @employee do |f|%>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :alias %>
<%= f.text_field :alias %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :pet %>
<%= collection_select(:employee, :dog_id, @dogs, :id, :name) %>
<%= f.submit %>
<% end  %>
